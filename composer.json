{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-pdo_sqlite": "*",
        "czproject/git-php": "^3.14",
        "diegoalvarezb/laravel-versioner": "^1.1",
        "doctrine/dbal": "^2.7",
        "eloquent/enumeration": "^5.1",
        "fideloper/proxy": "^4.0",
        "garygreen/pretty-routes": "^1.0",
        "graham-campbell/github": "^7.3",
        "graham-campbell/manager": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "igorescobar/jquery-mask-plugin": "^1.14",
        "knplabs/github-api": "^2.8",
        "kris/laravel-form-builder": "^1.14",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "patricktalmadge/bootstrapper": "^5.10",
        "php-http/guzzle6-adapter": "^1.1",
        "prettus/l5-repository": "^2.6",
        "prettus/laravel-validation": "^1.1",
        "spatie/laravel-permission": "^2.7",
        "symfony/event-dispatcher": "^4.1",
        "symfony/process": "^4.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "GitWrapper\\" : "src"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
